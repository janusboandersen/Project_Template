{
    "name": "CppProjectTemplateOnDocker",
    "image": "docker.io/janusboandersen/cppdevtest_ubuntu:0.8",
    "remoteEnv": {
        "PROJECT_NAME": "Project_Template",
        "PROJECT_BUILD_TYPE": "Debug",
        "PROJECT_BUILD_TESTS": "ON",
        "PROJECT_USE_CONAN": "ON"
    },
    "workspaceMount": "source=${localWorkspaceFolder},target=/root/dev/Project_Template,type=bind",
    "workspaceFolder": "/root/dev/Project_Template",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.cpptools",
                "ms-vscode.cpptools-extension-pack",
                "twxs.cmake",
                "ms-vscode.cmake-tools",
                "ms-python.python"
            ]
        }
    }
}